{
    "name": "zeropingheroes/lanager",
    "description": "A LAN party management web application",
    "authors": [
        {
            "name": "Olly Baker",
            "email": "ilumos@gmail.com",
            "role": "Developer"
        }
    ],
    "keywords": [
        "lanager",
        "lan",
        "party"
    ],
    "license": "AGPL-3.0-only",
    "config": {
        "platform": {
            "php": "8.0.13",
            "ext-bcmath": "8.0.13",
            "ext-curl": "8.0.13",
            "ext-dom": "8.0.13",
            "ext-fileinfo": "8.0.13",
            "ext-json": "8.0.13",
            "ext-libxml": "8.0.13",
            "ext-mbstring": "8.0.13",
            "ext-openssl": "8.0.13",
            "ext-pdo": "8.0.13",
            "ext-xml": "8.0.13",
            "ext-simplexml": "8.0.13",
            "ext-xmlwriter": "8.0.13",
            "ext-tokenizer": "8.0.13",
            "ext-zip": "8.0.13"
        },
        "optimize-autoloader": true,
        "preferred-install": "dist"
    },
    "require": {
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "laravel/framework": "^7.0",
        "laravel/socialite": "^5",
        "bandwidth-throttle/token-bucket": "^2.0",
        "graham-campbell/markdown": "^13",
        "league/csv": "^9.3",
        "socialiteproviders/steam": "^4",
        "syntax/steam-api": "^2.3",
        "diglactic/laravel-breadcrumbs": "^8.0"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "barryvdh/laravel-debugbar": "dev-master",
        "barryvdh/laravel-ide-helper": "^2.7",
        "behat/behat": "^3.7",
        "behat/mink": "^1.8",
        "behat/mink-goutte-driver": "^2",
        "friends-of-behat/mink-extension": "dev-goutte-client-4-compatibility",
        "squizlabs/php_codesniffer": "^3.6",
        "phpunit/phpunit": "^9.5",
        "fakerphp/faker": "^1.20"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:SwenVanZanten/MinkExtension.git"
        }
    ],
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "Zeropingheroes\\Lanager\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Features\\": "features/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan cache:clear",
            "@php artisan config:clear",
            "@php artisan view:clear",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ]
    }
}
