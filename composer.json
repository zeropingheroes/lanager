{
    "name": "zeropingheroes/lanager",
    "description": "A LAN party management web application",
    "authors": [
        {
            "name": "Olly Baker",
            "email": "ilumos@gmail.com",
            "role": "Developer"
        }
    ],
    "keywords": ["lanager", "lan", "party"],
    "license": "AGPL-3.0",
    "require": {
        "php": "^7.1.3",
        "barryvdh/laravel-debugbar": "dev-master",
        "davejamesmiller/laravel-breadcrumbs": "5.x",
        "doctrine/dbal": "^2.8",
        "fideloper/proxy": "^4.0",
        "gilg4mesh/monolog-mysql": "^0.1.8",
        "graham-campbell/markdown": "^10.2",
        "laravel/framework": "5.7.*",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "^1.0",
        "restcord/restcord": "^0.3.1",
        "socialiteproviders/discord": "^2.0",
        "socialiteproviders/steam": "^1.0",
        "syntax/steam-api": "2.0.*",
        "tucker-eric/eloquentfilter": "^1.2"
    },
    "require-dev": {
        "laravel/homestead": "^7.3",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },

    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "Zeropingheroes\\Lanager\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
