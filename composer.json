{
    "name": "zeropingheroes/lanager",
    "description": "A LAN party management web application",
    "authors": [
        {
            "name": "Olly Baker",
            "email": "ilumos@gmail.com",
            "role": "Developer"
        }
    ],
    "keywords": [
        "lanager",
        "lan",
        "party"
    ],
    "license": "AGPL-3.0-only",
    "config": {
        "platform": {
            "php": "7.4.0",
            "ext-bcmath": "7.4.3",
            "ext-curl": "7.4.3",
            "ext-dom": "20031129",
            "ext-fileinfo": "7.4.3",
            "ext-json": "7.4.3",
            "ext-libxml": "7.4.3",
            "ext-mbstring": "7.4.3",
            "ext-openssl": "7.4.3",
            "ext-pdo": "7.4.3",
            "ext-simplexml": "7.4.3",
            "ext-tokenizer": "7.4.3",
            "ext-zip": "7.4.3"
        },
        "optimize-autoloader": true,
        "preferred-install": "dist"
    },
    "require": {
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "laravel/framework": "^7.0",
        "laravel/tinker": "^2.0",
        "laravel/socialite": "^4.3",
        "bandwidth-throttle/token-bucket": "^2.0",
        "davejamesmiller/laravel-breadcrumbs": "5.x",
        "doctrine/dbal": "^2.8",
        "graham-campbell/markdown": "^12.0",
        "league/csv": "^9.3",
        "socialiteproviders/steam": "^3.0.1",
        "syntax/steam-api": "^2.1"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "barryvdh/laravel-debugbar": "dev-master",
        "barryvdh/laravel-ide-helper": "^2.7",
        "behat/behat": "^3.7",
        "behat/mink": "^1.8",
        "behat/mink-goutte-driver": "^1.2",
        "friends-of-behat/mink-extension": "^2.4",
        "squizlabs/php_codesniffer": "^3.6",
        "phpunit/phpunit": "^9.5",
        "fakerphp/faker": "^1.20"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "Zeropingheroes\\Lanager\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Features\\": "features/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan cache:clear",
            "@php artisan config:clear",
            "@php artisan view:clear",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ]
    }
}
